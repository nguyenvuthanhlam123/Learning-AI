import numpy as np

# Vectơ trong không gian 2D
vector_2d = np.array([3, 4])
print(f"Vectơ 2D: {vector_2d}")

# Tính độ dài (magnitude) của vectơ
do_dai = np.linalg.norm(vector_2d)
print(f"Độ dài vectơ: {do_dai}")

# Vectơ đơn vị (unit vector)
vector_don_vi = vector_2d / do_dai
print(f"Vectơ đơn vị: {vector_don_vi}")

# Hai vectơ ví dụ
vector_a = np.array([1, 2, 3])
vector_b = np.array([4, 5, 6])

# Tích vô hướng
tich_vo_huong = np.dot(vector_a, vector_b)
print(f"Tích vô hướng: {tich_vo_huong}")

# Tính cosine similarity
cosine_sim = tich_vo_huong / (np.linalg.norm(vector_a) * np.linalg.norm(vector_b))
print(f"Độ tương đồng cosine: {cosine_sim}")